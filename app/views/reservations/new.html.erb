<%= @reservation.start_date  %>
<%= @reservation.end_date %>

<%= form_for @reservation, url: reservation_create_path do |f|%>
        <%= f.label :start_date ,"開始日",class: hidden %>
        <%= f.date_field :start_date,,class: hidden %>
        <%= f.label :end_date ,"終了日",class: hidden %>
        <%= f.date_field :end_date,class: hidden %>
        <%= f.label :number_of_people,"人数",class: hidden %>
        <%= f.number_field :number_of_people,class: hidden %>
      <% end %>